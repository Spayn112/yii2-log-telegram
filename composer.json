{
    "name": "spayn/yii2-log-telegram",
    "description": "Telegram log target for Yii 2",
    "keywords": ["yii2", "yii", "telegram", "log", "logging"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/spayn/yii2-log-telegram"
    },
    "authors": [
        {
            "name": "Sergey Makinen",
            "email": "sergey@makinen.ru",
            "homepage": "https://makinen.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.2",
        "yiisoft/yii2-httpclient": "~2.0.0",
        "sergeymakinen/yii2-log-message": "^2.0"
    },
    "require-dev": {
        "sergeymakinen/yii2-tests": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "spayn\\yii\\log\\telegram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sergeymakinen\\yii\\telegramlog\\tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
